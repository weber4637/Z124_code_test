	-@if exist $(BIN_DIR)\MdePkg\Library\SmmServicesTableLib\SmmServicesTableLib\Makefile "$(MAKE)" $(MAKE_FLAGS) -f $(BIN_DIR)\MdePkg\Library\SmmServicesTableLib\SmmServicesTableLib\Makefile cleanall
