	-@if exist $(BIN_DIR)\MdePkg\Library\BaseLib\BaseLib\Makefile "$(MAKE)" $(MAKE_FLAGS) -f $(BIN_DIR)\MdePkg\Library\BaseLib\BaseLib\Makefile cleanall
	-@if exist $(BIN_DIR)\AmiModulePkg\Library\SmmAmiDebugLib\SmmAmiDebugLib\Makefile "$(MAKE)" $(MAKE_FLAGS) -f $(BIN_DIR)\AmiModulePkg\Library\SmmAmiDebugLib\SmmAmiDebugLib\Makefile cleanall
